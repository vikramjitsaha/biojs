<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
	<head>
		<meta http-equiv="content-type" content="text/html; charset={+IO.encoding+}" />
		<meta name="generator" content="BioJS Library" />
		{! Link.base = "../"; /* all generated links will be relative to this */ !}
		<title>Biojs {+data.name+} Registry</title>

		<link rel="stylesheet" type="text/css" href="style/style.css" />
		<link href="style/shThemeDefault.css" rel="stylesheet" type="text/css" />
		<link href="style/shCore.css" rel="stylesheet" type="text/css" />
		
		<script language="JavaScript" type="text/javascript" src="scripts/jquery-1.6.4.min.js"></script>
		<script src="scripts/shCore.js" type="text/javascript"></script>
		<script src="scripts/shBrushJScript.js" type="text/javascript"></script>
		
		<script language="JavaScript" type="text/javascript">
		  	SyntaxHighlighter.all();
		</script>
		
	</head>

	<body>
	<div id="main">
<!-- ============================== header ================================= -->	

	<div id="header">
      <div id="logo">
        <h1><a href="index.html"><span style="color:#FFF;border:none;">Bio</span><span class="alternate_colour">Js</span></a> {+data.name+}</h1>
      </div>
      <div id="menubar">
        <ul id="menu">
          <li><a href="{+data.alias+}.html">Overview</a></li>
          <li><a href="{+data.alias+}.options.html">Options</a></li>
          <li class="tab_selected"><a href="{+data.alias+}.events.html">Events</a></li>
          <li><a href="{+data.alias+}.methods.html">Methods</a></li>
        </ul>
      </div>
    </div>

<!-- ============================== site content ============================ -->	
	
	<div id="site_content">

	<div id="content"> 
    	<div class="accordion">
<if test="data.events.length">
{! var ownEvents = data.events.filter(function($){return $.memberOf != "Biojs" }).sort(makeSortby("name")); !}
<if test="ownEvents.length">
    <for each="member" in="ownEvents">

        <h3 class="method_name"><a href="#"><b>{+member.name+}</b></a></h3>
        <div class="method_desc"> <!-------------- Begin of event -->
        
            {+resolveLinks(member.desc)+}<br/>
            <if test="member.srcFile != data.srcFile">
            <br />
                <i>Defined in: </i> {+new Link().toSrc(member.srcFile)+}.
            </if>
            
            <if test="member.author"><br /><i>Author: </i>{+member.author+}.</if>
        
            <if test="member.params.length">
                <br/>
                <b>Parameters:</b>
                <ul>
                <for each="item" in="member.params">
                    <li>
                        {+((item.type)?"<span class=\"light fixedFont\">{"+(new Link().toSymbol(item.type))+"}</span> " : "")+}<b>{+item.name+}</b>
                        <if test="item.isOptional"><i>Optional<if test="item.defaultValue">, Default: {+item.defaultValue+}</if></i></if>
                        <br/>{+resolveLinks(item.desc)+}
                    </li>
                </for>
                </ul>
            </if>
            
            <if test="member.comment.getTag('eventData').length">
                <br/>
                <b>Returned data in the Biojs.Event object:</b>
                <ul>
                <for each="eventData" in="member.comment.getTag('eventData')">	
                    <li>
                        {+((eventData.type)?""+("<span class=\"light fixedFont\">{"+(new Link().toSymbol(eventData.type)+"}</span> ")) : "")+} 
                        <b>{+eventData.name+}</b>
                        <if test="eventData.isOptional"><i>Optional<if test="eventData.defaultValue">, Default: {+eventData.defaultValue+}</if></i></if>
                        <br/>{+resolveLinks(eventData.desc)+}
                    </li>
                </for>
                </ul>
            </if>
            
            <if test="member.example.length">
                {! var i = 0; !}
                <for each="example" in="member.example">
                   {! var counter = (member.example.length > 1)? (++i) : ""; !}
                   <br/>
                   <b>Example {+counter+}:</b>
                   <pre class="brush: js">{+example+}</pre>
                </for>
            </if>
            
        </div>	<!------------------- End of event -->
        
    </for>
 </if>
</if>
    	</div>	
	</div>	
	
	
</div>

<!-- ============================== footer ======================================= -->
    <br/>
	<div style="text-align: center; font-size: 0.75em;">
		Registry generated by <a href="http://code.google.com/p/jsdoc-toolkit/" target="_blank">JsDoc Toolkit</a> {+JSDOC.VERSION+} on {+new Date()+}
	</div>
	<br/>
	<br/>
	
</body>
</html>
